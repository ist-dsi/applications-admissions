{
  "name": "applications-admissions-frontend",
  "version": "1.0.0",
  "description": "Applications Admissions frontend application",
  "author": "DSI <dsi@tecnico.ulisboa.pt>",
  "private": true,
  "scripts": {
    "dev": "cross-env DEV=true webpack-dev-server --hot --inline --config build/webpack.conf.dev.js",
    "start": "cross-env CTX='dot' npm run dev",
    "dist": "mkdir -p ../webapp/applications-admissions; rm -rf ../webapp/applications-admissions/*; rm -rf dist; webpack --config build/webpack.conf.prod.js; cp -r dist/* ../webapp/applications-admissions/",
    "production": "npm run dist",
    "package": "mkdir -p ../webapp/applications-admissions; rm -rf ../webapp/applications-admissions/*; rm -rf dist; cross-env CTX='dot' DEV=false webpack --config build/webpack.conf.prod.js; cp -r dist/* ../webapp/applications-admissions/; mv ../webapp/applications-admissions/index.html ../webapp/applications-admissions/index.jsp",
    "staging": "npm run package",
    "lint": "eslint --ext .js,.vue src --fix",
    "analysis": "webpack --mode=production --display-reasons --json --config build/webpack.conf.prod.js > build/analysis.json",
    "clean": "rm -rf node_modules package-lock.json ../webapp/applications-admissions/* dist"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap": "^3.4.1",
    "jquery": "^3.5.1",
    "moment": "^2.26.0",
    "popper.js": "^1.16.1",
    "vue": "^2.6.0",
    "vue-recaptcha": "^1.3.0",
    "vue-router": "^3.3.0",
    "vuex": "^3.4.0",
    "vuex-i18n": "^1.13.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^9.0.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "copy-webpack-plugin": "^6.0.0",
    "cross-env": "^7.0.0",
    "css-loader": "^3.5.0",
    "eslint": "^6.0.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.0",
    "favicons-webpack-plugin": "^2.0.0",
    "file-loader": "^6.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-url": "^8.0.0",
    "prettier-eslint": "^10.1.0",
    "sass-loader": "^8.0.0",
    "url-loader": "^4.0.0",
    "vue-loader": "^15.9.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.0"
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 4.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
